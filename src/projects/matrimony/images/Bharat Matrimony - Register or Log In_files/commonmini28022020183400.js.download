function grandPhotoAccess(partnerId){
	if(($("#intAcceptShowphoto").prop('checked') == true)){
		var url = '//profile.'+DOMAINARRAY['domainnameshort']+'matrimony.com/mailbox/expressinterestaction.php';
		$.ajax({
			type:'GET',
			url:url,
			dataType:'json',
			data:{'PartnerId':partnerId,'ActionType':'PHACCESS'},
			success:function(result){
				if(result.RESPONSECODE == 1){
					$("#grandPhotoAccess").html('<span style="color:green;">Processing...</span>');
					setTimeout(function(){
						$("#grandPhotoAccess").slideUp("slow");
					}, 500);
				}else{
					$("#grandPhotoAccess").html('<span style="color:green;">Processing...</span>');
					setTimeout(function(){
						$("#grandPhotoAccess").slideUp("slow");
					}, 500);
				}
			}
		});
	}else{
		$('#photoChkMsg').show();
		$('#photoChkMsg').text('Select the option to submit');
		setTimeout(function(){
			$("#photoChkMsg").hide();
		}, 2000);
		return false;
	}
}

